<div id="get-bird">
    <div class="container" v-if="!cloudSuccess">
        <h1 class="text-center">Birds Table </h1>
        <hr/>
                
                <table id="all-birds" border="1">
                    <thead role="rowgroup">
                        <tr role="row">
                            <th role="columnheader"  style="width: 10%">Echo Name</th>
                            <th role="columnheader"  style="width: 10%">Bird ID</th>
                            <th role="columnheader"  style="width: 10%">Sex</th>
                            <th role="columnheader"  style="width: 10%">Male Parent</th>
                            <th role="columnheader"  style="width: 10%">Female Parent</th>
                            <th role="columnheader"  style="width: 10%">Age</th>
                            <th role="columnheader"  style="width: 10%">Breeder</th>
                            <th role="columnheader"  style="width: 10%">Current Nest</th>
                            <th role="columnheader"  style="width: 10%">Distance To Hopper</th>
                            <th role="columnheader"  style="width: 10%">Previous Nest</th>
                        </tr>
                    </thead>
    
                    <tbody role="rowgroup" id="warnings-table-body">
                    </tbody>
                    </table>

    </div>
</div>

