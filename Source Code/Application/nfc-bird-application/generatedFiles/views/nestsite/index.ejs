<h1>nestsite</h1>


<% if (result.length == 0) { %>
  <h4>
    0 nestsite
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>nestID</th>
			<th>createdBy</th>
			<th>nestDescription</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.nestID %>
					</td>
					<td>
						<%= item.createdBy %>
					</td>
					<td>
						<%= item.nestDescription %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/nestsite/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/nestsite/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/nestsite/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/nestsite/create">New</a>